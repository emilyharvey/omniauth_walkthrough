<!DOCTYPE html>
<html>
<head>
  <title>OmniauthTutorial</title>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
  <%= stylesheet_link_tag 'omniauth' %>
</head>
<body>
	<% flash.each do |name, msg| %>
      <%= content_tag :div, msg, :id => "flash_#{name}" %>
    <% end %>
	<%= yield %>

</body>
</html>
